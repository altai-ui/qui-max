    "object-hash": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
